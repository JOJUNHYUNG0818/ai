package javaBasic.ch04.sec01;

public class 확인문제4장 {

	public static void main(String[] args) {
		// oneHund();
		oneHundThree();

	}
	
	public static void oneHund()
	{
		for(int i=1; i<=100; i++)
		{
			System.out.println(i);
		}
	}
	
	public static void oneHundThree()
	{
		int sum=0;
		for(int i=1; i<=100; i++)
		{
			if(i%3==0)
			{
				sum +=i;
			}
		}
		System.out.println(sum);
	}

}
