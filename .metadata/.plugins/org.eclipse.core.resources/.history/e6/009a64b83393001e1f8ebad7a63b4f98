package javaBasic.ch04.sec01;

public class If¹® {

	public static void main(String[] args) {
		int score =  87;
		
		if(score>=90)
		{
			System.out.println("A");
		}
		else if(score>80 && score<90)
		{
			System.out.println("B");
		}
		else if(score>70 && score<80)
		{
			System.out.println("C");
		}
		else
		{
			System.out.println("D");
		}
	}
}
