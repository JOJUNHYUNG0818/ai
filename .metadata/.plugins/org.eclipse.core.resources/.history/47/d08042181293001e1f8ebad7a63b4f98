package javaBasic.ch03.sec02;
import java.util.Scanner;


public class 산술연산자 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		int kor = sc.nextInt();
		
		System.out.println("국어점수: "+kor);
		
		double rate = sc.nextDouble();
		System.out.println("비율: "+rate);

		
	}

}
