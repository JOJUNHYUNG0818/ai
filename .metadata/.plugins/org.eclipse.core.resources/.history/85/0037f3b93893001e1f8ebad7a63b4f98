package javaBasic.ch04.sec01;

public class Switch¹® {

	public static void main(String[] args) {
		char eng = 'A';
		
		switch(eng)
		{
		case 'A': System.out.println("Apple");
			break;
		case 'B': System.out.println("Bear");
			break;
		case 'C': System.out.println("Cat");
			break;
		
		
		
		}		
	}

}
