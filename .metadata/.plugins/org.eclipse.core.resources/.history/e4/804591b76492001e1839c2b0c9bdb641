package javaBasic.ch01.sec15;

public class DataType {

	public static void main(String[] args) {
		// byte short int long {1,2,4,8 바이트}
		// float double {4.8 바이트}

		
		float f3 = 10.23f;
		float f4 = 2.45f;
		
		float result = f3*f4;
		
		
		double d3=3.14;
		double d4 =12.65;
		double result1 = d3*d4;
				
		System.out.print(result+"\n");
		System.out.print(result1+"\n");
		
		double d10= 3.14;
		int chageDouble = (int)d10;
		System.out.println(d10+" "+chageDouble);
		
			
			
	}

}
