package javaBasic.ch03.sec02;

public class 확인문제3장 {

	public static void main(String[] args) {

		int x = 10;
		int y=20;
		int z = (++x)+(y--);
		
		System.out.println(z);
		
		
		int score= 85;
		
	    String result = (!(score>=90))? "가나":"나다";
		
	    int value = 356;
	    System.out.println(value/100*100);
	    
	    
	    int a =10;
	    int b= 5;
	    
	    System.out.println((a>7)&& (b<=5));
	    System.out.println((a%3 ==2)||(b%2!=1));
	    
	    double i = 5.0;
	    double j = 0.0;
	    double k = 5%j;
	    
	    if(i%j!=k) {
	    	System.out.println("0.0으로 나눌수 없습니다.");
	    }
	    else
	    {
	    	double result1 =k+10;
	    	System.out.println("결과 :"+result1);
	    	
	    }
	    
			}

}
