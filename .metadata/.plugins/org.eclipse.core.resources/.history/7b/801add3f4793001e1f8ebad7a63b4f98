package javaBasic.ch04.sec01;

public class while¹® {

	public static void main(String[] args) {
		//tenMul();
		//fiveMul();
		sumBy();
	}
	
	
	
	
	
	public static void tenMul()
	{
		int i = 10;
	
		while(i<=100)
		{
			System.out.println(i);
			i+=10;
		}
	}
	
	
	public static void fiveMul()
	{
		int i=0;
		
		while(i<10)
		{
			i++;
			if(i%5==0)
			{
				continue;
			}
			System.out.println(i);
		}
		
	}
	
	
	public static void sumBy() 
	{
		int i =1;
		int sum =1;
		while(i<100)
		{
			i++;
			sum +=i;
			if(sum>=3000)
				break;
		}
		System.out.println(sum);
		System.out.println(i);
	}

}


