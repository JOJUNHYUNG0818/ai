package javaBasic.ch01.sec12;

public class Calculator {

	public static void main(String[] args) {
		
		
		String dept = "알바생";
		boolean sex = true;
		double tex = 0.03;
		
		String name = "홍길동";
		String name1 = "이순신";
		String name2 = "이경미";
		String name3 = "정현희";
		String name4 = "최수연";
		
		String phoneNumber = "010-1234-1234";
		String phoneNumber1 = "010-234-4567";
		String phoneNumber2 = "010-987-7456";
		String phoneNumber3 = "010-8744-3334";
		String phoneNumber4 = "010-333-7777";
		
		String accountNumber = "국민 123-12-1234";
		String accountNumber1 = "우리 321-32-4321";
		String accountNumber2 = "신한 123-555-7878";
		String accountNumber3 = "국민 555-65-8989";
		String accountNumber4 = "카카오 777-454-1231";
		
		int basicPay = 50000;
		int basicPay1 = 20000;
		int basicPay2 = 15000;
		int basicPay3 = 10000;
		int basicPay4 = 9800;
		
		
		
		
		System.out.println(name+"\n"+basicPay+"\n"+phoneNumber+"\n"+accountNumber+"\n"+dept+"\n"+sex+"\n"+tex+"\n");
		System.out.println(name1+"\n"+basicPay1+"\n"+phoneNumber1+"\n"+accountNumber1+"\n"+dept+"\n"+sex+"\n"+tex+"\n");
		System.out.println(name2+"\n"+basicPay2+"\n"+phoneNumber2+"\n"+accountNumber2+"\n"+dept+"\n"+sex+"\n"+tex+"\n");
		System.out.println(name3+"\n"+basicPay3+"\n"+phoneNumber3+"\n"+accountNumber3+"\n"+dept+"\n"+sex+"\n"+tex+"\n");
		System.out.println(name4+"\n"+basicPay4+"\n"+phoneNumber4+"\n"+accountNumber4+"\n"+dept+"\n"+sex+"\n"+tex+"\n");
	}

}