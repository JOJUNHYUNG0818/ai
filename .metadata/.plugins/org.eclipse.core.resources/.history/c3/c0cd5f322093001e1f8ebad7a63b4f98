package javaBasic.ch03.sec02;

public class 대입연산자102 {

	public static void main(String[] args) {
		int count = 100;
		count +=10; //count = count + 10;
		
		System.out.println(count);
		
		count -=20;
		System.out.println(count);
		
		count *= 5; //count = count*5;
		System.out.println(count);
		
		count /= 10;
		System.out.println(count);
		
		count %= 5;
		System.out.println(count);
		
		boolean b1 = true;
		System.out.println(b1);
		
		b1 &= false;
		System.out.println(b1);
		
		b1 |= true;
		System.out.println(b1);
	}

}
